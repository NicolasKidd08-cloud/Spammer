        connections.spin = RunService.Heartbeat:Connect(function()
            if states.spin and rootPart then
                rootPart.CFrame = rootPart.CFrame * CFrame.Angles(0, math.rad(15), 0)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
