-- INFINITE JUMP (WORKS!)
createButton(content, "Infinite Jump", "‚ôæÔ∏è", 5, function(s, st)
    states.infiniteJump = not states.infiniteJump
    
    if states.infiniteJump then
        connections.infiniteJump = UserInputService.JumpRequest:Connect(function()
            if states.infiniteJump then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.infiniteJump then connections.infiniteJump:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- REALISTIC HIP THRUSTING HUMP! (FIXED!)
createButton(content, "HUMP Selected (REALISTIC HIP THRUST)", "üòà", 6, function(s, st)
    states.hump = not states.hump
    
    if states.hump then
        if not selectedPlayer then
            states.hump = false
            notify("‚ö†Ô∏è", "Select a player first!", 2)
            return
        end
        
        -- Store original CFrame
        local originalRootC0
        local originalWaistC0
        
        if isR15 then
            -- R15: We need to manipulate the Waist joint
            local waist = character:FindFirstChild("UpperTorso"):FindFirstChild("Waist")
            if waist then
                originalWaistC0 = waist.C0
            end
        else
            -- R6: Manipulate Root joint
            local root = rootPart:FindFirstChild("RootJoint")
            if root then
                originalRootC0 = root.C0
            end
        end
        
        connections.hump = RunService.Heartbeat:Connect(function()
            if not states.hump or not selectedPlayer then
                -- Reset on stop
                if isR15 and character:FindFirstChild("UpperTorso") then
                    local waist = character.UpperTorso:FindFirstChild("Waist")
                    if waist and originalWaistC0 then
                        waist.C0 = originalWaistC0
                    end
                elseif not isR15 and rootPart:FindFirstChild("RootJoint") and originalRootC0 then
                    rootPart.RootJoint.C0 = originalRootC0
                end
                return
            end
            
            if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayer.Character.HumanoidRootPart
                
                -- FAST hip thrust motion (realistic humping)
                local time = tick()
                local thrust = math.sin(time * 10) -- Fast thrusting (10 times per second)
                local offset = thrust * 1.2 -- Back and forth distance
                
                -- Position behind target with thrust offset
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 2 + offset) * CFrame.Angles(math.rad(10), 0, 0)
                
                -- Hip thrust animation - tilt torso forward/back
                if isR15 and character:FindFirstChild("UpperTorso") then
                    local waist = character.UpperTorso:FindFirstChild("Waist")
                    if waist then
                        -- Tilt upper body forward and back
                        waist.C0 = originalWaistC0 * CFrame.Angles(math.rad(thrust * 15), 0, 0)
                    end
                elseif not isR15 and rootPart:FindFirstChild("RootJoint") then
                    -- R6 hip thrust
                    rootPart.RootJoint.C0 = originalRootC0 * CFrame.Angles(math.rad(thrust * 15), 0, 0)
                end
                
                -- Raise both arms up
                if isR15 then
                    -- R15 Arms
                    if character:FindFirstChild("RightUpperArm") then
                        local rightShoulder = character.UpperTorso:FindFirstChild("RightShoulder")
                        if rightShoulder then
                            rightShoulder.C0 = CFrame.new(1, 0.5, 0) * CFrame.Angles(math.rad(90), 0, 0)
                        end
                    end
                    if character:FindFirstChild("LeftUpperArm") then
                        local leftShoulder = character.UpperTorso:FindFirstChild("LeftShoulder")
                        if leftShoulder then
                            leftShoulder.C0 = CFrame.new(-1, 0.5, 0) * CFrame.Angles(math.rad(90), 0, 0)
                        end
                    end
                else
                    -- R6 Arms
                    if torso:FindFirstChild("Right Shoulder") then
                        torso["Right Shoulder"].C0 = CFrame.new(1, 0.5, 0) * CFrame.Angles(math.rad(90), 0, 0)
                    end
                    if torso:FindFirstChild("Left Shoulder") then
                        torso["Left Shoulder"].C0 = CFrame.new(-1, 0.5, 0) * CFrame.Angles(math.rad(90), 0, 0)
                    end
                end
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("üòà", "HUMPING " .. selectedPlayer.Name .. "!", 2)
    else
        if connections.hump then connections.hump:Disconnect() end
        
        -- Reset arms and torso
        if isR15 then
            if character:FindFirstChild("UpperTorso") then
                local rightShoulder = character.UpperTorso:FindFirstChild("RightShoulder")
                local leftShoulder = character.UpperTorso:FindFirstChild("LeftShoulder")
                if rightShoulder then
                    rightShoulder.C0 = CFrame.new(1, 0.5, 0, 0, 0, 1, 0, 1, 0, -1, 0, 0)
                end
                if leftShoulder then
                    leftShoulder.C0 = CFrame.new(-1, 0.5, 0, 0, 0, -1, 0, 1, 0, 1, 0, 0)
                end
            end
        else
            if torso:FindFirstChild("Right Shoulder") then
                torso["Right Shoulder"].C0 = CFrame.new(1, 0.5, 0, 0, 0, 1, 0, 1, 0, -1, 0, 0)
            end
            if torso:FindFirstChild("Left Shoulder") then
                torso["Left Shoulder"].C0 = CFrame.new(-1, 0.5, 0, 0, 0, -1, 0, 1, 0, 1, 0, 0)
            end
        end
        
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- TP TO SELECTED (WORKS!)
createButton(content, "TP to Selected", "üìç", 7, function(s, st)
    if selectedPlayer and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local targetRoot = selectedPlayer.Character.HumanoidRootPart
        rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
        notify("‚úÖ", "Teleported to " .. selectedPlayer.Name, 1.5)
    else
        notify("‚ö†Ô∏è", "Select a player first!", 2)
    end
end)

-- FOLLOW PLAYER (WORKS!)
createButton(content, "Follow Selected", "üéØ", 8, function(s, st)
    states.follow = not states.follow
    
    if states.follow then
        if not selectedPlayer then
            states.follow = false
            notify("‚ö†Ô∏è", "Select a player first!", 2)
            return
        end
        
        connections.follow = RunService.Heartbeat:Connect(function()
            if not states.follow or not selectedPlayer then return end
            
            if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayer.Character.HumanoidRootPart
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("üéØ", "Following " .. selectedPlayer.Name, 2)
    else
        if connections.follow then connections.follow:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- FLING SUPER (WORKS!)
createButton(content, "FLING (Spin Super Fast)", "üí•", 9, function(s, st)
    states.fling = not states.fling
    
    if states.fling then
        -- Make character spin super fast to fling others
        local spin = Instance.new("BodyAngularVelocity")
        spin.Name = "Spin"
        spin.MaxTorque = Vector3.new(0, math.huge, 0)
        spin.AngularVelocity = Vector3.new(0, 100, 0) -- Crazy spin speed
        spin.P = 5000
        spin.Parent = rootPart
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("üí•", "FLING MODE!", 2)
    else
        if rootPart:FindFirstChild("Spin") then
            rootPart.Spin:Destroy()
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- SPIN CHARACTER (WORKS!)
