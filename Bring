        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- SEIZURE (WORKS!)
createButton(content, "Seizure Mode (Shake Violently)", "üì≥", 11, function(s, st)
    states.seizure = not states.seizure
    
    if states.seizure then
        connections.seizure = RunService.Heartbeat:Connect(function()
            if states.seizure and rootPart then
                rootPart.CFrame = rootPart.CFrame * CFrame.new(
                    math.random(-1, 1) * 0.5,
                    math.random(-1, 1) * 0.5,
                    math.random(-1, 1) * 0.5
                ) * CFrame.Angles(
                    math.rad(math.random(-30, 30)),
                    math.rad(math.random(-30, 30)),
                    math.rad(math.random(-30, 30))
                )
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.seizure then connections.seizure:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- SIT (WORKS!)
