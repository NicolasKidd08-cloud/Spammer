-- PLATFORM (WORKS!)
createButton(content, "Platform Under You", "üü¶", 13, function(s, st)
    states.platform = not states.platform
    
    if states.platform then
        local platform = Instance.new("Part")
        platform.Name = "Platform"
        platform.Size = Vector3.new(8, 0.5, 8)
        platform.Anchored = true
        platform.Transparency = 0.5
        platform.BrickColor = BrickColor.new("Bright red")
        platform.Material = Enum.Material.Neon
        platform.CanCollide = true
        platform.Parent = Workspace
        
        connections.platform = RunService.Heartbeat:Connect(function()
            if states.platform and rootPart then
                platform.CFrame = CFrame.new(rootPart.Position.X, rootPart.Position.Y - 4, rootPart.Position.Z)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.platform then connections.platform:Disconnect() end
        if Workspace:FindFirstChild("Platform") then
            Workspace.Platform:Destroy()
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- HEADLESS (WORKS!)
createButton(content, "Headless", "üë§", 14, function(s, st)
    states.headless = not states.headless
    
    if states.headless and character:FindFirstChild("Head") then
        character.Head.Transparency = 1
        if character.Head:FindFirstChild("face") then
            character.Head.face.Transparency = 1
        end
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if character:FindFirstChild("Head") then
            character.Head.Transparency = 0
            if character.Head:FindFirstChild("face") then
                character.Head.face.Transparency = 0
            end
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- ORBIT PLAYER (WORKS!)
createButton(content, "Orbit Selected Player", "üåç", 15, function(s, st)
    states.orbit = not states.orbit
    
    if states.orbit then
        if not selectedPlayer then
            states.orbit = false
            notify("‚ö†Ô∏è", "Select a player first!", 2)
            return
        end
        
        local angle = 0
        connections.orbit = RunService.Heartbeat:Connect(function()
            if not states.orbit or not selectedPlayer then return end
            
            if selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetRoot = selectedPlayer.Character.HumanoidRootPart
                
                angle = angle + 0.1
                local x = math.cos(angle) * 8
                local z = math.sin(angle) * 8
                
                rootPart.CFrame = targetRoot.CFrame * CFrame.new(x, 2, z)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
        notify("üåç", "Orbiting " .. selectedPlayer.Name, 2)
    else
        if connections.orbit then connections.orbit:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- BACKFLIP SPAM (WORKS!)
createButton(content, "Backflip Spam", "ü§∏", 16, function(s, st)
    states.backflip = not states.backflip
    
    if states.backflip then
        connections.backflip = RunService.Heartbeat:Connect(function()
            if states.backflip and rootPart then
                rootPart.CFrame = rootPart.CFrame * CFrame.Angles(math.rad(10), 0, 0)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.backflip then connections.backflip:Disconnect() end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- HELICOPTER (WORKS!)
createButton(content, "Helicopter (Fly + Spin)", "üöÅ", 17, function(s, st)
    states.helicopter = not states.helicopter
    
    if states.helicopter then
        local bodyVel = Instance.new("BodyVelocity")
        bodyVel.MaxForce = Vector3.new(9e9, 9e9, 9e9)
        bodyVel.Velocity = Vector3.new(0, 50, 0)
        bodyVel.Parent = rootPart
        
        connections.helicopter = RunService.Heartbeat:Connect(function()
            if states.helicopter and rootPart then
                rootPart.CFrame = rootPart.CFrame * CFrame.Angles(0, math.rad(25), 0)
            end
        end)
        
        st.Text = "ON"
        s.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        st.TextColor3 = Color3.fromRGB(255, 255, 255)
    else
        if connections.helicopter then connections.helicopter:Disconnect() end
        for _, obj in pairs(rootPart:GetChildren()) do
            if obj:IsA("BodyVelocity") then obj:Destroy() end
        end
        st.Text = "OFF"
        s.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        st.TextColor3 = Color3.fromRGB(200, 200, 200)
    end
end)

-- Entrance Animation
mainFrame.Size = UDim2.new(0, 0, 0, 0)
playerListFrame.Size = UDim2.new(0, 0, 0, 0)

wait(0.1)

TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 500, 0, 500)
}):Play()

wait(0.2)

TweenService:Create(playerListFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back), {
    Size = UDim2.new(0, 230, 0, 500)
}):Play()

wait(1)
notify("üíÄ WORKING CHAOS", "All Features Fixed!", 3)
wait(1)
