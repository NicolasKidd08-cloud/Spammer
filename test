-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local connections = {}

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ESP SYSTEM (See targets through walls - THIS WORKS!)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local espEnabled = false
local espFolder = Instance.new("Folder")
espFolder.Name = "NexaESP"
espFolder.Parent = game.CoreGui

local function createESPBox(targetPlayer)
    pcall(function()
        if not targetPlayer.Character then return end
        local hrp = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        if not hrp then return end
        
        -- Billboard for name + distance
        local billboard = Instance.new("BillboardGui")
        billboard.Name = targetPlayer.Name .. "_ESP"
        billboard.Adornee = hrp
        billboard.Size = UDim2.new(0, 100, 0, 40)
        billboard.StudsOffset = Vector3.new(0, 3, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = espFolder
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, 0, 0.5, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = targetPlayer.Name
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        nameLabel.TextStrokeTransparency = 0
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextSize = 14
        nameLabel.Parent = billboard
        
        local distLabel = Instance.new("TextLabel")
        distLabel.Name = "Dist"
        distLabel.Size = UDim2.new(1, 0, 0.5, 0)
        distLabel.Position = UDim2.new(0, 0, 0.5, 0)
        distLabel.BackgroundTransparency = 1
        distLabel.Text = "0m"
        distLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
        distLabel.TextStrokeTransparency = 0
        distLabel.Font = Enum.Font.Gotham
        distLabel.TextSize = 12
        distLabel.Parent = billboard
        
        -- Highlight
        local highlight = Instance.new("Highlight")
        highlight.Name = targetPlayer.Name .. "_HL"
        highlight.Adornee = targetPlayer.Character
        highlight.FillColor = Color3.fromRGB(255, 255, 0)
        highlight.FillTransparency = 0.7
        highlight.OutlineColor = Color3.fromRGB(255, 200, 0)
        highlight.OutlineTransparency = 0
        highlight.Parent = espFolder
    end)
end

local function updateESP()
    if not espEnabled then return end
    
    local myHRP = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if not myHRP then return end
    
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= player and p.Character then
            local hrp = p.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                -- Create if doesn't exist
                if not espFolder:FindFirstChild(p.Name .. "_ESP") then
                    createESPBox(p)
                end
                
                -- Update distance
                local billboard = espFolder:FindFirstChild(p.Name .. "_ESP")
                if billboard then
                    local dist = (myHRP.Position - hrp.Position).Magnitude
                    local distLabel = billboard:FindFirstChild("Dist")
                    if distLabel then
                        distLabel.Text = math.floor(dist) .. "m"
                    end
                end
            end
        end
    end
end

local function clearESP()
    for _, child in pairs(espFolder:GetChildren()) do
        child:Destroy()
    end
end

local function toggleESP(enabled)
    espEnabled = enabled
    if not enabled then
        clearESP()
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- BASE TIMER FINDER (Find bases about to unlock!)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function findUnlockingBases()
    local results = {}
    
    pcall(function()
        -- Look for base lock timers in workspace
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("TextLabel") or obj:IsA("SurfaceGui") then
                local text = ""
                if obj:IsA("TextLabel") then
                    text = obj.Text or ""
                end
                
                -- Check for timer patterns (like "0:15" or "15s")
                if text:match("%d+:%d+") or text:match("%d+s") then
                    table.insert(results, {
                        object = obj,
                        text = text,
                        position = obj:IsDescendantOf(workspace) and obj:FindFirstAncestorOfClass("Model")
                    })
                end
            end
        end
    end)
    
    return results
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ANTI-RAGDOLL (Prevents getting knocked down)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local antiRagdollEnabled = false

local function setAntiRagdoll(enabled)
    antiRagdollEnabled = enabled
    
    if connections.antiRagdoll then
        connections.antiRagdoll:Disconnect()
        connections.antiRagdoll = nil
    end
    
    if enabled then
        connections.antiRagdoll = RunService.Heartbeat:Connect(function()
            pcall(function()
                local char = player.Character
                if char then
                    local humanoid = char:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
                        humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
                    end
                    
                    -- Disable ragdoll constraints
                    for _, v in pairs(char:GetDescendants()) do
                        if v:IsA("BallSocketConstraint") or v:IsA("HingeConstraint") then
                            v.Enabled = false
                        end
                    end
                end
            end)
        end)
        print("âœ… Anti-Ragdoll ON")
    else
        print("âŒ Anti-Ragdoll OFF")
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- CLICK TP (Small teleports that might work)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local clickTPEnabled = false

local function setupClickTP()
    if connections.clickTP then
        connections.clickTP:Disconnect()
        connections.clickTP = nil
    end
    
    if clickTPEnabled then
        local mouse = player:GetMouse()
        connections.clickTP = mouse.Button1Down:Connect(function()
            if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
                local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
                if hrp and mouse.Hit then
                    -- Small teleport (under detection threshold hopefully)
                    local targetPos = mouse.Hit.Position + Vector3.new(0, 3, 0)
                    local distance = (hrp.Position - targetPos).Magnitude
                    
                    if distance < 20 then -- Only short TPs
                        hrp.CFrame = CFrame.new(targetPos)
                    else
                        print("âš ï¸ Too far! Keep it under 20 studs")
                    end
                end
            end
        end)
        print("âœ… Click TP ON (Ctrl+Click, max 20 studs)")
    end
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- SPEED BOOST (Small, hopefully undetected)
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local speedBoostEnabled = false
local originalWalkSpeed = 16

local function setSpeedBoost(enabled, multiplier)
    multiplier = multiplier or 1.3 -- Only 30% boost to avoid detection
    
    pcall(function()
        local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if enabled then
                originalWalkSpeed = humanoid.WalkSpeed
                humanoid.WalkSpeed = 16 * multiplier
                print("âœ… Speed Boost ON (" .. (multiplier * 100) .. "%)")
            else
                humanoid.WalkSpeed = 16
                print("âŒ Speed Boost OFF")
            end
        end
    end)
    
    speedBoostEnabled = enabled
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- GUI
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local function createGUI()
    local playerGui = player:WaitForChild("PlayerGui")
    
    if playerGui:FindFirstChild("NexaUltimate") then
        playerGui:FindFirstChild("NexaUltimate"):Destroy()
    end
    
    local gui = Instance.new("ScreenGui")
    gui.Name = "NexaUltimate"
    gui.ResetOnSpawn = false
    gui.Parent = playerGui
    
    local main = Instance.new("Frame")
    main.Size = UDim2.new(0, 300, 0, 520)
    main.Position = UDim2.new(0.5, -150, 0.5, -260)
    main.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
    main.BorderSizePixel = 0
    main.Parent = gui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = main
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(255, 50, 50)
    stroke.Thickness = 2
    stroke.Parent = main
    
    -- Title
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 50)
    title.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    title.Text = "ğŸ§  NEXA ULTIMATE"
    title.Font = Enum.Font.GothamBlack
    title.TextSize = 16
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.Parent = main
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 12)
    titleCorner.Parent = title
    
    local titleFix = Instance.new("Frame")
    titleFix.Size = UDim2.new(1, 0, 0, 15)
    titleFix.Position = UDim2.new(0, 0, 1, -15)
    titleFix.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    titleFix.BorderSizePixel = 0
    titleFix.Parent = title
    
    -- Info box
    local infoBox = Instance.new("TextLabel")
    infoBox.Size = UDim2.new(1, -20, 0, 60)
    infoBox.Position = UDim2.new(0, 10, 0, 55)
    infoBox.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    infoBox.Text = "âš ï¸ Noclip/Fly WON'T work - server-side AC!\nâœ… Use QUANTUM CLONER + INVISIBILITY CLOAK\nâœ… These tools help you execute REAL methods"
    infoBox.Font = Enum.Font.Gotham
    infoBox.TextSize = 10
    infoBox.TextColor3 = Color3.fromRGB(255, 200, 100)
    infoBox.TextWrapped = true
    infoBox.Parent = main
    
    local infoCorner = Instance.new("UICorner")
    infoCorner.CornerRadius = UDim.new(0, 8)
    infoCorner.Parent = infoBox
    
    local function createBtn(text, yPos, color)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(0, 275, 0, 35)
        btn.Position = UDim2.new(0, 12, 0, yPos)
        btn.BackgroundColor3 = color or Color3.fromRGB(45, 45, 55)
        btn.Text = text
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 12
        btn.TextColor3 = Color3.fromRGB(255, 255, 255)
        btn.Parent = main
        
        local c = Instance.new("UICorner")
        c.CornerRadius = UDim.new(0, 8)
        c.Parent = btn
        
        return btn
    end
    
    local function createLabel(text, yPos)
        local lbl = Instance.new("TextLabel")
        lbl.Size = UDim2.new(1, -20, 0, 18)
        lbl.Position = UDim2.new(0, 10, 0, yPos)
        lbl.BackgroundTransparency = 1
        lbl.Text = text
        lbl.Font = Enum.Font.GothamBold
        lbl.TextSize = 10
        lbl.TextColor3 = Color3.fromRGB(100, 100, 110)
        lbl.TextXAlignment = Enum.TextXAlignment.Left
        lbl.Parent = main
    end
    
    -- Sections
    createLabel("â”â”â” VISION (Works!) â”â”â”", 125)
    local espBtn = createBtn("ğŸ‘ï¸ Player ESP: OFF", 143, Color3.fromRGB(80, 80, 150))
    
    createLabel("â”â”â” DEFENSE â”â”â”", 185)
    local antiRagBtn = createBtn("ğŸ›¡ï¸ Anti-Ragdoll: OFF", 203)
    
    createLabel("â”â”â” MOVEMENT (Risky) â”â”â”", 245)
    local speedBtn = createBtn("âš¡ Speed Boost (1.3x): OFF", 263)
    local clickTPBtn = createBtn("ğŸ–±ï¸ Click TP (Ctrl+Click): OFF", 303)
    
    createLabel("â”â”â” REAL METHODS â”â”â”", 345)
    
    local methodsBox = Instance.new("TextLabel")
    methodsBox.Size = UDim2.new(1, -20, 0, 130)
    methodsBox.Position = UDim2.new(0, 10, 0, 363)
    methodsBox.BackgroundColor3 = Color3.fromRGB(25, 35, 25)
    methodsBox.Text = [[âœ… QUANTUM CLONER METHOD:
1. Buy Quantum Cloner (Rebirth 5)
2. Stand against target's wall
3. Place clone INSIDE their base
4. Press "Change Clone" = you're in!

âœ… BODY SWAP METHOD:
1. Wait for timer <7 seconds
2. Body swap with owner
3. They get trapped, you steal!

âœ… COIL ESCAPE:
Spam coil after stealing for speed boost]]
    methodsBox.Font = Enum.Font.Gotham
    methodsBox.TextSize = 9
    methodsBox.TextColor3 = Color3.fromRGB(150, 255, 150)
    methodsBox.TextWrapped = true
    methodsBox.TextXAlignment = Enum.TextXAlignment.Left
    methodsBox.TextYAlignment = Enum.TextYAlignment.Top
    methodsBox.Parent = main
    
    local methodsCorner = Instance.new("UICorner")
    methodsCorner.CornerRadius = UDim.new(0, 8)
    methodsCorner.Parent = methodsBox
    
    -- Draggable
    local dragging, dragInput, mousePos, framePos
    title.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            mousePos = input.Position
            framePos = main.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then dragging = false end
            end)
        end
    end)
    title.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then dragInput = input end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - mousePos
            main.Position = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
        end
    end)
    
    return {
        espBtn = espBtn,
        antiRagBtn = antiRagBtn,
        speedBtn = speedBtn,
        clickTPBtn = clickTPBtn
    }
end

