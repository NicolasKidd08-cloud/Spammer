local ui = createGUI()

-- ESP Toggle
ui.espBtn.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    ui.espBtn.Text = espEnabled and "ğŸ‘ï¸ Player ESP: ON" or "ğŸ‘ï¸ Player ESP: OFF"
    ui.espBtn.BackgroundColor3 = espEnabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(80, 80, 150)
    toggleESP(espEnabled)
    
    if espEnabled then
        if connections.espUpdate then connections.espUpdate:Disconnect() end
        connections.espUpdate = RunService.Heartbeat:Connect(updateESP)
    else
        if connections.espUpdate then 
            connections.espUpdate:Disconnect() 
            connections.espUpdate = nil
        end
    end
end)

-- Anti-Ragdoll Toggle
ui.antiRagBtn.MouseButton1Click:Connect(function()
    antiRagdollEnabled = not antiRagdollEnabled
    ui.antiRagBtn.Text = antiRagdollEnabled and "ğŸ›¡ï¸ Anti-Ragdoll: ON" or "ğŸ›¡ï¸ Anti-Ragdoll: OFF"
    ui.antiRagBtn.BackgroundColor3 = antiRagdollEnabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(45, 45, 55)
    setAntiRagdoll(antiRagdollEnabled)
end)

-- Speed Toggle
ui.speedBtn.MouseButton1Click:Connect(function()
    speedBoostEnabled = not speedBoostEnabled
    ui.speedBtn.Text = speedBoostEnabled and "âš¡ Speed Boost (1.3x): ON" or "âš¡ Speed Boost (1.3x): OFF"
    ui.speedBtn.BackgroundColor3 = speedBoostEnabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(45, 45, 55)
    setSpeedBoost(speedBoostEnabled, 1.3)
end)

-- Click TP Toggle
ui.clickTPBtn.MouseButton1Click:Connect(function()
    clickTPEnabled = not clickTPEnabled
    ui.clickTPBtn.Text = clickTPEnabled and "ğŸ–±ï¸ Click TP (Ctrl+Click): ON" or "ğŸ–±ï¸ Click TP (Ctrl+Click): OFF"
    ui.clickTPBtn.BackgroundColor3 = clickTPEnabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(45, 45, 55)
    setupClickTP()
end)

-- Respawn handling
player.CharacterAdded:Connect(function()
    wait(1)
    if antiRagdollEnabled then
        setAntiRagdoll(true)
    end
    if speedBoostEnabled then
        setSpeedBoost(true, 1.3)
    end
end)

